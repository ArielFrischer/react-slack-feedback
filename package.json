{
  "name": "react-slack-feedback",
  "version": "1.1.1",
  "description":
    "React component for gathering user feedback to send to slack.",
  "main": "lib/feedback.js",
  "author":
    "Mark Murray <mark.murray.ie@gmail.com> (https://github.com/markmur)",
  "license": "MIT",
  "scripts": {
    "start": "nf start --wrap",
    "build": " NODE_ENV=production webpack --config webpack.config.prod.js",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "test": "xo"
  },
  "lint-staged": {
    "src/*.js": ["prettier --single-quote --write", "git add", "xo"]
  },
  "precommit": {
    "run": ["lint-staged"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markmur/react-slack-feedback.git"
  },
  "keywords": ["react", "slack", "feedback"],
  "bugs": {
    "url": "https://github.com/markmur/react-slack-feedback/issues"
  },
  "homepage": "https://github.com/markmur/react-slack-feedback",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.41",
    "@babel/preset-env": "^7.0.0-beta.41",
    "@babel/preset-react": "^7.0.0-beta.41",
    "@babel/preset-stage-1": "^7.0.0-beta.41",
    "autoprefixer": "^6.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.15.2",
    "cloudinary": "^1.4.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "foreman": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^7.0.0",
    "multer": "^1.2.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.10.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "xo": "^0.20.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^3.2.1",
    "prop-types": "^15.5.10",
    "styled-components": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15 || ^16",
    "react-dom": "^0.14 || ^15 || ^16"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "envs": ["browser", "node"],
    "prettier": true,
    "space": true,
    "plugins": ["react"],
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  }
}
